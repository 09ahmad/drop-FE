{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/assets/(.*)",
        "headers": {
          "cache-control": "public, max-age=31536000, immutable"
        }
      },
      {
        "src": "/(.*)",
        "dest": "/index.html",
        "headers": {
          "cache-control": "no-cache"
        }
      }
    ],
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "https://drop-be-six.vercel.app/api/v1/$1"
      }
    ]
  }